alias xcode="open -a Xcode *.xcworkspace"

alias rn="nice -n 5 react-native"
alias rni="nice -n 5 react-native run-ios"
alias rnr="nice -n 5 react-native run-android"
alias rnpkg="nice -n 5 ./node_modules/react-native/packager/launchPackager.command ; exit;"

export PATH=$PATH:/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin
